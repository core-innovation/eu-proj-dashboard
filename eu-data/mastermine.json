  "projectInfo": {
    "title": "European Mining in the Green and Digital Era",
    "acronym": "MASTERMINE",
    "grantNumber": "101091895",
    "grantType": "HORIZON Innovation Actions",
    "maxAmount": "€11,963,524.50",
